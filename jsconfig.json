{
    "compilerOptions": {
        "baseUrl": "./",
        "paths": {
            "@/*": ["src/*"],
            "@components/*": ["src/components/*"],
            "@assets/*": ["src/assets/*"],
            "@icons/*": ["src/assets/icons/*"],
            "@pages/*": ["src/pages/*"],
            "@apis/*": ["src/apis/*"],
            "@hooks/*": ["src/hooks/*"],
            "@utils/*": ["src/utils/*"],
            "@constants/*": ["src/constants/*"],
            "@validations/*": ["src/validations/*"]
        }
    },
    "include": ["src/**/*"]
}